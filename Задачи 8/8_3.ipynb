    "def main():\n",
    "    n = 4\n",
    "    k = 9\n",
    "\n",
    "    def S(n, k):\n",
    "        if n == k:\n",
    "            return 1\n",
    "        elif k == 0:\n",
    "            return 0\n",
    "        elif 0 < k and k <= n:\n",
    "            return S(n - 1, k - 1) + k * S(n - 1, k)\n",
    "\n",
    "    print(S(15, 9))\n",
    "\n",
    "if __name__ == \"__main__\":\n",
    "    main()    "
   ]
