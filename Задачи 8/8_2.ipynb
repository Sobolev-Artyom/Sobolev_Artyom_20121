   "source": [
    "def main():\n",
    "    import itertools\n",
    "    r = []\n",
    "    stuff = [1,2,3,4]\n",
    "    for t in range(1, len(stuff) + 1):\n",
    "        for subset in itertools.combinations(stuff, t):\n",
    "            f = set(subset)\n",
    "            r.append(f)\n",
    "    print(r, len(r))\n",
    "\n",
    "if __name__==\"__main__\":\n",
    "    main() \n"
   ]
  }
